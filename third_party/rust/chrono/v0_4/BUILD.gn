import("//build/rust/cargo_crate.gni")

cargo_crate("lib") {
  crate_name = "chrono"
  epoch = "0.4"
  crate_type = "rlib"
  crate_root = "crate/src/lib.rs"
  sources = [
    "//express/third_party/rust/chrono/v0_4/crate/benches/chrono.rs",
    "//express/third_party/rust/chrono/v0_4/crate/benches/serde.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/date.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/datetime/mod.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/datetime/rustc_serialize.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/datetime/serde.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/datetime/tests.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/format/locales.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/format/mod.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/format/parse.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/format/parsed.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/format/scan.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/format/strftime.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/lib.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/month.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/naive/date.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/naive/datetime/mod.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/naive/datetime/rustc_serialize.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/naive/datetime/serde.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/naive/datetime/tests.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/naive/internals.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/naive/isoweek.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/naive/mod.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/naive/time/mod.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/naive/time/rustc_serialize.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/naive/time/serde.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/naive/time/tests.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/offset/fixed.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/offset/local/mod.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/offset/local/stub.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/offset/local/tz_info/mod.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/offset/local/tz_info/parser.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/offset/local/tz_info/rule.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/offset/local/tz_info/timezone.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/offset/local/unix.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/offset/local/windows.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/offset/mod.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/offset/utc.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/oldtime.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/round.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/traits.rs",
    "//express/third_party/rust/chrono/v0_4/crate/src/weekday.rs",
    "//express/third_party/rust/chrono/v0_4/crate/tests/dateutils.rs",
    "//express/third_party/rust/chrono/v0_4/crate/tests/wasm.rs",
  ]
  inputs = [
    "//express/third_party/rust/chrono/v0_4/crate/.github/pull_request_template.md",
    "//express/third_party/rust/chrono/v0_4/crate/CHANGELOG.md",
    "//express/third_party/rust/chrono/v0_4/crate/README.md",
  ]

  # Unit tests skipped. Generate with --with-tests to include them.
  build_native_rust_unit_tests = false
  edition = "2018"
  cargo_pkg_version = "0.4.24"
  cargo_pkg_name = "chrono"
  cargo_pkg_description = "Date and time library for Rust"
  library_configs -= [ "//build/config/compiler:chromium_code" ]
  library_configs += [ "//build/config/compiler:no_chromium_code" ]
  executable_configs -= [ "//build/config/compiler:chromium_code" ]
  executable_configs += [ "//build/config/compiler:no_chromium_code" ]
  deps = [
    "//express/third_party/rust/num_integer/v0_1:lib",
    "//express/third_party/rust/num_traits/v0_2:lib",
    "//third_party/rust/serde/v1:lib",
  ]
  if (!is_win) {
    deps += [ "//express/third_party/rust/iana_time_zone/v0_1:lib" ]
  }
  if (is_win) {
    deps += [ "//third_party/rust/winapi/v0_3:lib" ]
  }
  features = [
    "clock",
    "iana-time-zone",
    "serde",
    "std",
    "winapi",
  ]
}
