  epoch = "1"
  crate_type = "rlib"
  crate_root = "crate/src/lib.rs"
  sources = [
    "//express/third_party/rust/thiserror/v1/crate/build.rs",
    "//express/third_party/rust/thiserror/v1/crate/src/aserror.rs",
    "//express/third_party/rust/thiserror/v1/crate/src/display.rs",
    "//express/third_party/rust/thiserror/v1/crate/src/lib.rs",
    "//express/third_party/rust/thiserror/v1/crate/src/provide.rs",
    "//express/third_party/rust/thiserror/v1/crate/tests/compiletest.rs",
    "//express/third_party/rust/thiserror/v1/crate/tests/test_backtrace.rs",
    "//express/third_party/rust/thiserror/v1/crate/tests/test_deprecated.rs",
    "//express/third_party/rust/thiserror/v1/crate/tests/test_display.rs",
    "//express/third_party/rust/thiserror/v1/crate/tests/test_error.rs",
    "//express/third_party/rust/thiserror/v1/crate/tests/test_expr.rs",
    "//express/third_party/rust/thiserror/v1/crate/tests/test_from.rs",
    "//express/third_party/rust/thiserror/v1/crate/tests/test_generics.rs",
    "//express/third_party/rust/thiserror/v1/crate/tests/test_lints.rs",
    "//express/third_party/rust/thiserror/v1/crate/tests/test_option.rs",
