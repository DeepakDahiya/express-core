import("//build/rust/cargo_crate.gni")

cargo_crate("lib") {
  crate_name = "rand"
  epoch = "0.7"
  crate_type = "rlib"
  crate_root = "crate/src/lib.rs"
  sources = [
    "//express/third_party/rust/rand/v0_7/crate/benches/generators.rs",
    "//express/third_party/rust/rand/v0_7/crate/benches/misc.rs",
    "//express/third_party/rust/rand/v0_7/crate/benches/seq.rs",
    "//express/third_party/rust/rand/v0_7/crate/benches/weighted.rs",
    "//express/third_party/rust/rand/v0_7/crate/examples/monte-carlo.rs",
    "//express/third_party/rust/rand/v0_7/crate/examples/monty-hall.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/distributions/bernoulli.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/distributions/binomial.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/distributions/cauchy.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/distributions/dirichlet.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/distributions/exponential.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/distributions/float.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/distributions/gamma.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/distributions/integer.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/distributions/mod.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/distributions/normal.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/distributions/other.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/distributions/pareto.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/distributions/poisson.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/distributions/triangular.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/distributions/uniform.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/distributions/unit_circle.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/distributions/unit_sphere.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/distributions/utils.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/distributions/weibull.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/distributions/weighted/alias_method.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/distributions/weighted/mod.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/distributions/ziggurat_tables.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/lib.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/prelude.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/rngs/adapter/mod.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/rngs/adapter/read.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/rngs/adapter/reseeding.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/rngs/entropy.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/rngs/mock.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/rngs/mod.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/rngs/small.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/rngs/std.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/rngs/thread.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/seq/index.rs",
    "//express/third_party/rust/rand/v0_7/crate/src/seq/mod.rs",
  ]
  inputs = [
    "//express/third_party/rust/rand/v0_7/crate/.github/ISSUE_TEMPLATE/compile-issue.md",
    "//express/third_party/rust/rand/v0_7/crate/.github/ISSUE_TEMPLATE/feature_request.md",
    "//express/third_party/rust/rand/v0_7/crate/.github/ISSUE_TEMPLATE/other.md",
    "//express/third_party/rust/rand/v0_7/crate/CHANGELOG.md",
    "//express/third_party/rust/rand/v0_7/crate/README.md",
    "//express/third_party/rust/rand/v0_7/crate/SECURITY.md",
  ]

  # Unit tests skipped. Generate with --with-tests to include them.
  build_native_rust_unit_tests = false
  edition = "2018"
  cargo_pkg_version = "0.7.3"
  cargo_pkg_authors = "The Rand Project Developers, The Rust Project Developers"
  cargo_pkg_name = "rand"
  cargo_pkg_description =
      "Random number generators and other randomness functionality."
  library_configs -= [ "//build/config/compiler:chromium_code" ]
  library_configs += [ "//build/config/compiler:no_chromium_code" ]
  executable_configs -= [ "//build/config/compiler:chromium_code" ]
  executable_configs += [ "//build/config/compiler:no_chromium_code" ]
  deps = [
    "//express/third_party/rust/getrandom/v0_1:lib",
    "//express/third_party/rust/rand_chacha/v0_2:lib",
    "//express/third_party/rust/rand_core/v0_5:lib",
    "//express/third_party/rust/rand_pcg/v0_2:lib",
  ]
  if (!is_win) {
    deps += [ "//third_party/rust/libc/v0_2:lib" ]
  }
  aliased_deps = {
    getrandom_package = "//express/third_party/rust/getrandom/v0_1:lib__rlib"
  }
  features = [
    "alloc",
    "getrandom",
    "getrandom_package",
    "libc",
    "rand_pcg",
    "small_rng",
    "std",
  ]
}
